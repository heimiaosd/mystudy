// 读入n个整数 计算它们的平均值

#include<stdio.h>

int main()
{
    int n;
    double sum;
    scanf("%d",&n);
    for(int i=0; i<n; i++)
    {
        int temp;
        scanf("%d",&temp);
        sum += temp;
    }
    printf("%lf\n",sum/n);
    return 0;
}

